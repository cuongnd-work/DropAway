CCEffect %{
  techniques:
  - name: depthmask
    passes:
    - vert: legacy/main-functions/general-vs:vert
      frag: depthmask-fs:frag
      rasterizerState:
        cullMode: none          # render cả 2 mặt
        priority: 1000          # render trước Floor
      depthStencilState:
        depthTest: true
        depthWrite: true         # ghi depth
        depthFunc: less          # depth nhỏ hơn thì pass
      blendState:
        targets:
        - blend: true
          blendSrc: src_alpha
          blendDst: one_minus_src_alpha
%}

CCProgram depthmask-fs %{
  precision highp float;
  #include <legacy/output>

  in vec3 v_position;

  vec4 frag() {
      return CCFragOutput(vec4(0.0, 0.0, 0.0, 0.0));    // trong suốt hoàn toàn
  }
}%
